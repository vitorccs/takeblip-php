{
    "name": "vitorccs/takeblip-php",
    "description": "SDK PHP para a API WhatsApp da Take Blip",
    "homepage": "https://github.com/vitorccs/takeblip-php",
    "license": "GPL-3.0",
    "keywords": [
        "whatsapp",
        "take",
        "blip",
        "take blip"
    ],
    "authors": [
        {
            "name": "Vitor Siqueira",
            "email": "vitorccsiqueira@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.3.3|^7.0.1",
        "ramsey/uuid": "^3.7|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "TakeBlip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TakeBlip\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox"
    }
}
